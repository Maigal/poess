{  
    "name": "POE Search Saver",  
    "version": "1.0.0",  
    "description": "Path of Exile Search Saver",  
    "browser_action": {  
      "default_popup": "main.html",
      "default_icon": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
      }
    },
    "icons": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    },
    "permissions": ["tabs", "storage"],
    "manifest_version": 2  
  }  